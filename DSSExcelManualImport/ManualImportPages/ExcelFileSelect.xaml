<Window x:Class="DSSExcel.ExcelFileSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DSSExcel"
        mc:Ignorable="d"
        Title="ExcelFileSelect" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" x:Name="FileNameTextBox" Height="20" Margin="10,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="738" Background="WhiteSmoke" IsReadOnly="True"/>
        <Button Content="..." x:Name="FileSelectButton" HorizontalAlignment="Left" Margin="753,13,0,0" VerticalAlignment="Top" Width="29" Click="FileSelectButton_Click"/>
        <Button Content="Next" x:Name="NextButton" HorizontalAlignment="Left" Margin="683,386,0,0" VerticalAlignment="Top" Width="99" Height="23" Click="NextButton_Click"/>
        <Button Content="Back" x:Name="BackButton" HorizontalAlignment="Left" Margin="10,386,0,0" VerticalAlignment="Top" Width="99" Height="23" Click="BackButton_Click"/>

    </Grid>
</Window>
