<UserControl x:Class="DssExcel.ExcelRangeSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DssExcel" xmlns:sg="clr-namespace:SpreadsheetGear.Windows.Controls;assembly=SpreadsheetGear2017.Windows.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
    <sg:WorkbookView Grid.Row="3" Grid.ColumnSpan="2" x:Name="ExcelView" Margin="0,10,0,10" RangeSelectionBorderBrush="#D013D400" RangeSelectionChanged="ExcelView_RangeSelectionChanged"/>

  </Grid>
</UserControl>
